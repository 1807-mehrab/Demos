<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
	"http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
	
<hibernate-mapping>
	<class name="com.revature.domain.Cave" table="CAVE">
		<id name="cave_id" column="cave_id">
			<generator class="increment"/>
		</id>
		
		<property name="cave_name" column="cave_name"/>
		<property name="max_bears" column="max_bears"/>
	</class>
</hibernate-mapping>